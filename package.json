{
  "name": "cliftin",
  "description": "A read-only CLI for liftin",
  "version": "0.0.0",
  "author": "Nick Christensen",
  "bin": {
    "cliftin": "./bin/run.js"
  },
  "bugs": "https://github.com/nickchristensen/cliftin/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "better-sqlite3": "^12.6.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.4",
    "kysely": "^0.28.11",
    "tty-table": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^24.10.13",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/nickchristensen/cliftin",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "cliftin",
    "dirname": "cliftin",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "exercises": {
        "description": "Exercise catalog and performance"
      },
      "programs": {
        "description": "Program hierarchy and planning"
      },
      "workouts": {
        "description": "Workout history and detail"
      }
    }
  },
  "repository": "nickchristensen/cliftin",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "db:sync": "cp /Users/nick/Library/Containers/com.nstrm.Bello/Data/Library/Application\\ Support/Liftin/BelloDataModel.* ./data",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
